<div class="container">

<%= form_with model: @event do |f| %>
    <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>
    <div>
        <%= f.label :date %>
        <%= f.date_field  :date %>
    </div>
    <div>
        <%= f.label :location %>
        <%= f.text_field :location%>
    </div>
    <div>
        <%= f.submit%>
    </div>
<% end %>
</div>