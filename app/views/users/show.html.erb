<h3> My Events </h3>
<ul> 
    <% current_user.events.all.each do |event| %>
        <%= render partial: "shared/event", object: event %>
    <% end %>
</ul>

<h3> Past Events </h3>
<ul>
    <% current_user.attended_events.past.each do |event|%>
        <%= render partial: "shared/event", object: event %>
    <% end %>
</ul>

<h3> Upcoming Events </h3>
<ul>
    <% current_user.attended_events.future.each do |event|%>
        <%= render partial: "shared/event", object: event %>
    <% end %>
</ul>